                    "computing": "actual",
                    "element": {
                        "percentage": {
                            "related": {
                                "ghwrapper": {
                                    "events": {
                                        "type": "CreateEvent",
                                        "payload": {
                                            "ref_type": "branch"
                                        }
                                    }
                                },
                                "window": 86400
                            }
                        }
                    },
                    "event": {
                        "pivotal": {
                            "activity": {
                                "highlight": "started"
                            }
                        }
                    },
                    "scope": {
